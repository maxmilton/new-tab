{
  "name": "new-tab",
  "version": "0.18.0",
  "description": "âš¡ A high performance new tab page that gets you where you need to go faster.",
  "repository": "maxmilton/new-tab",
  "homepage": "https://github.com/maxmilton/new-tab",
  "author": "Max Milton <max@happycode.ltd>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "pnpm run prebuild && NODE_ENV=production node build.mjs",
    "dev": "NODE_ENV=development node build.mjs",
    "lint": "pnpm run lint:css && pnpm run lint:js && pnpm run lint:ts",
    "lint:css": "echo 'TODO: CSS linting'; exit 0",
    "lint:js": "eslint --ignore-path .gitignore --ext .cjs,.js,.mjs,.ts .",
    "lint:ts": "tsc --project test",
    "prebuild": "rm -rf dist; cp -r static dist",
    "test": "c8 --all --include=src --reporter=text --reporter=lcovonly uvu -r tsm test '\\.test\\.ts$'",
    "test-e2e": "node -r tsm test/e2e/index.ts",
    "zip": "sh -c 'test -z \"$(git status --porcelain)\"' && pnpm run lint && pnpm run test && pnpm run build && cd dist && zip ../chrome-extension.zip *"
  },
  "__pnpm": {
    "overrides": {
      "stage1": "link:../stage1"
    }
  },
  "dependencies": {
    "stage1": "0.0.11"
  },
  "devDependencies": {
    "@types/chrome": "0.0.160",
    "@types/csso": "4.2.0",
    "@types/jsdom": "16.2.13",
    "@types/node": "16.11.4",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "c8": "7.10.0",
    "csso": "4.2.0",
    "ekscss": "0.0.9",
    "esbuild": "0.13.9",
    "esbuild-minify-templates": "0.6.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-unicorn": "37.0.1",
    "git-ref": "0.3.0",
    "jsdom": "18.0.0",
    "kleur": "4.1.4",
    "playwright-chromium": "1.16.1",
    "prettier": "2.4.1",
    "tsm": "2.1.2",
    "typescript": "4.4.4",
    "uvu": "0.5.2"
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all"
  }
}
