{
  "name": "new-tab",
  "version": "0.6.2",
  "description": "A high performance new tab page that gets you where you need to go faster.",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "svelte",
    "maxmilton"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup -c",
    "prebuild": "rm -rf dist && mkdir dist",
    "lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.html src test",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci",
    "pretest:dev": "yarn lint",
    "test:dev": "yarn test --coverage --notify --watch"
  },
  "dependencies": {
    "raven-js": "3.24.2"
  },
  "devDependencies": {
    "@types/chrome": "0.0.64",
    "@wearegenki/dev": "0.12.0",
    "@wearegenki/test": "0.10.0",
    "clean-css": "4.1.11",
    "codecov": "3.0.1",
    "eslint": "4.19.1",
    "html-minifier": "3.5.15",
    "jest": "22.4.3",
    "postcss": "6.0.22",
    "postcss-load-config": "1.2.0",
    "rollup": "0.58.2",
    "rollup-plugin-buble": "0.19.2",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-svelte": "4.1.0",
    "rollup-plugin-uglify": "3.0.0",
    "stylelint": "9.2.0",
    "svelte": "2.4.1",
    "svelte-jest": "0.1.0"
  },
  "engines": {
    "node": ">= 9.10.0"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
