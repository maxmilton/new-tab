{
  "name": "new-tab",
  "version": "0.7.3",
  "description": "âš¡ A high performance new tab page that gets you where you need to go faster.",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "svelte",
    "maxmilton"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup -c",
    "prebuild": "rm -rf dist && mkdir dist",
    "lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.html src test",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci",
    "pretest:dev": "yarn lint",
    "test:dev": "yarn test --notify --watch"
  },
  "dependencies": {
    "raven-js": "3.26.3"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.52",
    "@minna-ui/eslint-config": "0.3.0",
    "@minna-ui/jest-config": "0.4.0",
    "@minna-ui/stylelint-config": "0.4.0",
    "@minna-ui/svelte-preprocess-markup": "0.4.0",
    "@minna-ui/svelte-preprocess-style": "0.3.0",
    "@types/chrome": "0.0.69",
    "autoprefixer": "8.6.5",
    "babel-eslint": "8.2.5",
    "clean-css": "4.1.11",
    "codecov": "3.0.3",
    "eslint": "5.0.1",
    "jest": "23.3.0",
    "postcss": "6.0.23",
    "postcss-import": "11.1.0",
    "postcss-nested": "3.0.0",
    "rollup": "0.62.0",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-svelte": "4.2.1",
    "rollup-plugin-terser": "1.0.1",
    "sinon-chrome": "2.3.2",
    "stylelint": "9.3.0",
    "svelte": "2.9.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
