{
  "name": "new-tab",
  "version": "0.6.3",
  "description": "A high performance new tab page that gets you where you need to go faster.",
  "author": "Max Milton <max@wearegenki.com>",
  "license": "MIT",
  "private": true,
  "keywords": [
    "google-chrome",
    "ntp",
    "chrome-extension",
    "chrome",
    "svelte",
    "maxmilton"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "APP_RELEASE=$(git describe --always --dirty=\"-dev\") NODE_ENV=production rollup -c",
    "prebuild": "rm -rf dist && mkdir dist",
    "lint:css": "stylelint --cache --cache-location=/tmp/ \"**/*.{html,css}\"",
    "lint:js": "eslint --ignore-path .gitignore --ext .js,.html src test",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test-ci": "yarn test --coverage --runInBand --ci",
    "pretest:dev": "yarn lint",
    "test:dev": "yarn test --coverage --notify --watch"
  },
  "dependencies": {
    "raven-js": "3.25.1"
  },
  "devDependencies": {
    "@minna-ui/eslint-config": "0.0.0",
    "@minna-ui/jest-config": "0.0.0",
    "@minna-ui/postcss-config": "0.0.0",
    "@minna-ui/stylelint-config": "0.0.0",
    "@minna-ui/svelte-preprocess-markup": "0.0.0",
    "@minna-ui/svelte-preprocess-style": "0.0.0",
    "@types/chrome": "0.0.65",
    "clean-css": "4.1.11",
    "codecov": "3.0.2",
    "eslint": "4.19.1",
    "jest": "22.4.3",
    "postcss": "6.0.22",
    "rollup": "0.59.0",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-svelte": "4.1.0",
    "rollup-plugin-uglify": "3.0.0",
    "sinon-chrome": "2.3.2",
    "stylelint": "9.2.0",
    "svelte": "2.6.1"
  },
  "browserslist": [
    "last 2 Chrome versions"
  ]
}
