/**
 * Brave dark theme
 *
 * @see https://github.com/brave/brave-core/blob/78c9dd0bb7916c29f324f84e31f0fd7720cc4b27/browser/themes/theme_properties.cc
 */

:root {
  --s: 0 0 0 1px #000; // shadow
  --b: #0c0c17; // background
  --t: #fff; // text
  --c1: #000; // bookmark node hover background, hr, button border, search input border
  --c2: #303443; // bookmarks bar, menu dropdown, "load more" button
  --c3: #fff; // icons, empty folder text, text fallback (headings, etc.)
}
